{
	"name": "mv-file",
	"version": "0.0.0",
	"description": "A simple and flexible utility for moving files and directories with support for glob patterns, concurrent operations, and event handling.",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"test": "jest",
		"build": "rimraf dist && tsup-node src/index.ts --format=esm,cjs --dts"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"keywords": [
		"mv",
		"move",
		"move file",
		"move directory",
		"glob"
	],
	"author": "Mervin <mengqing723@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Marinerer/accjs.git",
		"directory": "libs/mv-file"
	},
	"bugs": {
		"url": "https://github.com/Marinerer/accjs/issues"
	},
	"homepage": "https://github.com/Marinerer/accjs",
	"dependencies": {
		"@types/fs-extra": "^11.0.4",
		"fast-glob": "^3.3.2",
		"fs-extra": "^11.2.0",
		"pathe": "^1.1.2"
	}
}
